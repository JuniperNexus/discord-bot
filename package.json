{
    "name": "discord-bot",
    "version": "1.1.0",
    "description": "A feature-rich Discord bot for the Juniper Nexus guild.",
    "license": "MIT",
    "main": "src/index.ts",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "tsc",
        "database-types": "supabase gen types typescript --project-id \"qpmtvzspevuvztptbwwr\" --schema public > ./src/types/Supabase/DatabaseGenerated.ts",
        "postdatabase-types": "pnpm format:fix",
        "dev": "tsx --watch src/index.ts",
        "format:fix": "prettier --write . --list-different",
        "knip": "knip",
        "knip:fix": "knip --fix",
        "lint": "eslint . --ext .ts",
        "prestart": "pnpm build",
        "start": "node dist/index.js",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "dependencies": {
        "@supabase/supabase-js": "^2.43.4",
        "cli-color": "^2.0.4",
        "dayjs": "^1.11.11",
        "discord.js": "^14.15.2",
        "dotenv": "^16.4.5",
        "supabase": "^1.169.5",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.3.0",
        "@commitlint/config-conventional": "^19.2.2",
        "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
        "@types/cli-color": "^2.0.6",
        "@types/node": "^20.12.12",
        "@typescript-eslint/eslint-plugin": "^7.10.0",
        "@typescript-eslint/parser": "^7.10.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "husky": "^9.0.11",
        "knip": "^5.16.0",
        "lint-staged": "^15.2.4",
        "prettier": "^3.2.5",
        "prettier-plugin-package-perfection": "^1.1.0",
        "prettier-plugin-sort-json": "^4.0.0",
        "rimraf": "^5.0.7",
        "tsx": "^4.11.0",
        "typescript": "^5.4.5",
        "validate-branch-name": "^1.3.0"
    },
    "engines": {
        "node": ">=19.9.0"
    }
}